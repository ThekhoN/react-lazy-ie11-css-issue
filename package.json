{
	"name": "new-app-poc",
	"version": "1.0.0",
	"description": "",
	"jest": {
		"transform": {
			"^.+\\.js?$": "babel-jest"
		},
		"roots": [
			"<rootDir>/src"
		],
		"setupTestFrameworkScriptFile": "<rootDir>/config/jest.setup-tests.js",
		"testPathIgnorePatterns": [
			"<rootDir>/node_modules",
			"__fixtures__"
		],
		"collectCoverage": true,
		"coverageReporters": [
			"html"
		],
		"collectCoverageFrom": [
			"**/redux/**",
			"!**/__fixtures__/**",
			"!**/__snapshots__/**"
		],
		"globals": {
			"statements": 50,
			"branches": 50,
			"functions": 50,
			"lines": 50
		},
		"verbose": true,
		"moduleNameMapper": {
			"\\.(scss|css|jpg|png|gif)$": "<rootDir>/config/empty-module.js"
		}
	},
	"scripts": {
		"start": "npm run dev",
		"dev": "cross-env NODE_ENV=development BABEL_ENV=development webpack-dev-server --config config/webpack.config.dev.js --hot --history-api-fallback --open"
	},
	"author": "",
	"license": "ISC",
