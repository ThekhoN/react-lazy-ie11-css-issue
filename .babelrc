{
	"env": {
		"testing": {
			"presets": ["@babel/preset-env", "@babel/preset-react"],
			"plugins": ["@babel/plugin-proposal-class-properties"]
		},
		"development": {
			"presets": [
				"@babel/preset-env",
				"@babel/preset-react"
			],
			"plugins": ["@babel/plugin-proposal-class-properties"]
		},
		"production": {
			"presets": [ 
				[
					"@babel/preset-env",
					{
					"targets": {
						"ie": "11"
						}
					}
				], "@babel/preset-react"],
			"plugins": ["@babel/plugin-proposal-class-properties"]
		}
	}
}
